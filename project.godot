; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="HorrorQuest_v0"
run/main_scene="res://scenes/Main Menu/MainMenu.tscn"
config/icon="res://icon.png"

[audio]

output_latency.web=150

[autoload]

GlobalMusic="*res://global_data/GlobalMusic.tscn"
GlobalEffects="*res://global_data/GlobalEffects.tscn"
GlobalData="*res://global_data/global_data.gd"
PlayerData="*res://global_data/player_data.gd"
SceneChanger="*res://global_data/SceneHelper/SceneChanger.tscn"
GlobalMessageBox="*res://objects/Interactive/Messages/GlobalMessageBox.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/dpi/allow_hidpi=true
mouse_cursor/custom_image="res://sprites/mouse_cursor/regular.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

Cursor=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
